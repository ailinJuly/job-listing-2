<div class="container">
    <div class="col-md-6 col-centered">
        <div class="form-area">
            <h3>投交简历到 <%= @job.title %></h3>
            <hr>
            <div class="form-group">
                <%= simple_form_for [@job,@resume] do |f| %>
                    <%= f.input :content %>
                    <%= f.input :attachment %>
                    <%= f.submit "送出" ,:class => "btn btn-primary pull-right"%>
                <% end %>
            </div>
        </div>
    </div>
</div>
